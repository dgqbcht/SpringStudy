<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="person1" name="p1,pp1" class="org.dgqbcht.springstudy.bean.Person1" />
    <bean name="p2,pp2" class="org.dgqbcht.springstudy.bean.Person1" />

    <bean id="person2" class="org.dgqbcht.springstudy.bean.Person2">
        <property name="myByte">
            <value>1</value>
        </property>
        <property name="myShort">
            <value>2</value>
        </property>
        <property name="myInt">
            <value>3</value>
        </property>
        <property name="myLong">
            <value>4</value>
        </property>
        <property name="myFloat">
            <value>5.0</value>
        </property>
        <property name="myDouble">
            <value>6.0</value>
        </property>
        <property name="myChar">
            <value>C</value>
        </property>
        <property name="myBoolean">
            <value>false</value>
        </property>
        <property name="myString">
            <value>haha</value>
        </property>
    </bean>
    <bean id="person2_new" class="org.dgqbcht.springstudy.bean.Person2"
          p:myByte="7" p:myShort="8" p:myInt="9" p:myLong="10"
          p:myFloat="11.0" p:myDouble="12.0" p:myChar="e" p:myBoolean="true" p:myString="hehe"
    />

</beans>
<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="person4" class="org.dgqbcht.springstudy.simplebean.Person4">
        <constructor-arg value="zhangsan"/>
        <constructor-arg value="10"/>
    </bean>
    <bean id="person4_s" class="org.dgqbcht.springstudy.simplebean.Person4">
        <constructor-arg type="java.lang.String" value="zhangsan"/>
    </bean>
    <bean id="person4_i" class="org.dgqbcht.springstudy.simplebean.Person4">
        <constructor-arg type="long" value="10"/>
    </bean>
</beans>